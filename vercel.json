{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "MISTRAL_KEY": "@mistral_key",
    "LECHAT_API_KEY": "@lechat_api_key",
    "GITHUB_TOKEN": "@github_token"
  }
}