{
  "name": "ouicomply-mcp-server",
  "description": "OuiComply MCP Server for Le Chat Integration",
  "version": "1.0.0",
  "runtime": "python3.12",
  "entrypoint": "mcp_server_flask.py",
  "port": 8000,
  "environment": {
    "PYTHONPATH": "/app",
    "MISTRAL_KEY": "Gr9ZoQBFhi3tk85GChM6NjfcfOgg0XPW",
    "LECHAT_API_URL": "https://api.lechat.ai",
    "LECHAT_API_KEY": "your_lechat_api_key_here",
    "MCP_SERVER_NAME": "ouicomply-mcp",
    "MCP_SERVER_VERSION": "1.0.0",
    "MCP_SERVER_PORT": "8000",
    "LOG_LEVEL": "INFO",
    "FLASK_ENV": "production"
  },
  "dependencies": "requirements_mcp.txt",
  "health_check": "/health",
  "endpoints": {
    "mcp": "/mcp",
    "sse": "/mcp/sse",
    "health": "/health",
    "lechat": "/lechat/integration"
  },
  "capabilities": {
    "mcp_protocol": true,
